{
  "version": 2,
  "name": "qr-access-system",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "framework": null,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "https://qr-jxts.onrender.com",
    "GENERATE_SOURCEMAP": "false",
    "CI": "false",
    "SKIP_PREFLIGHT_CHECK": "true",
    "VERCEL": "1"
  }
}